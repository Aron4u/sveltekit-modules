		"gen-ext": "node tools/graphql-modules-codegen-ext"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "1.21.3",
		"@graphql-codegen/graphql-modules-preset": "1.2.4",
		"@graphql-codegen/introspection": "1.18.1",
		"@graphql-codegen/typescript": "1.21.1",
		"@graphql-codegen/typescript-document-nodes": "1.17.10",
		"@graphql-codegen/typescript-operations": "1.17.15",
		"@graphql-codegen/typescript-resolvers": "1.19.0",
		"@sveltejs/adapter-node": "next",
		"@sveltejs/kit": "next",
		"@types/yargs": "16.0.1",
		"@typescript-eslint/eslint-plugin": "4.22.0",
		"@typescript-eslint/parser": "4.22.0",
		"@urql/devtools": "^2.0.3",
		"eslint": "7.24.0",
		"eslint-config-prettier": "8.2.0",
		"eslint-plugin-svelte3": "3.1.2",
		"graphql-helix": "1.3.0",
		"graphql-modules": "^1.4.2",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "2.2.0",
		"svelte": "3.37.0",
		"svelte-preprocess": "4.7.2",
		"tslib": "2.2.0",
		"typescript": "4.2.4",
		"vite": "2.2.1",
		"@graphql-tools/load-files": "6.3.2"
	},
	"dependencies": {
		"@urql/core": "2.0.0",
		"@urql/exchange-request-policy": "0.1.1",
		"@urql/svelte": "1.2.1",
		"graphql": "15.4.0"
	},
	"resolutions": {
		"graphql": "15.4.0"
	},
	"volta": {
		"node": "14.16.0",
		"yarn": "1.22.10"
	},
	"type": "module"
