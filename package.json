{
	"name": "jyc",
	"version": "0.0.1",
	"license": "MIT",
	"scripts": {
		"gen": "graphql-codegen --config codegen.yml",
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"start": "svelte-kit start",
		"lint": "prettier --check . && eslint --ignore-path .gitignore .",
